<%

dim num 
num = 5000

res = FormatNumber(num,,, -2)

response.write(customNumberFormat(num)) 


function customNumberFormat(number)
    getres = FormatNumber(number,,, -1)
    finalRes = split(getres,".")
    customNumberFormat = finalRes(0)
end function 


%>

//CSS For anchor tag
 .countlink {
     text-decoration: none ;
     font-size:20px;
     color:#007acc;
  }
.countlink:hover
  {
    text-decoration:none;
    color:#4d79ff;
    cursor:pointer;
   }
